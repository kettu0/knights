

@{
    ViewData["Title"] = "Lands";
}
<div class="text-center">
    <h2 class="display-4">Knights from lands with 'land' in land's name</h2>
    <table>
        <tr>
            <th><strong>Id</strong></th>
            <th><strong>Name</strong></th>
            <th><strong>Land</strong></th>
        </tr>
        @foreach (var knight in Model)
        {
    <tr>
        <th>@knight.Id</th>
        <th>@knight.Name</th>
        <th>@knight.Land</th>

    </tr>

        }
    </table>
</div>